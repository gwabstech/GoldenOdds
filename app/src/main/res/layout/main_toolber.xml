    android:gravity="center_vertical"
    tools:ignore="Overdraw"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:background="#36B1AD"
